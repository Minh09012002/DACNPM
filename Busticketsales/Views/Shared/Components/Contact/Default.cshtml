@using Busticketsales.Models;
@model IList<ContactQuality>


<!-- Contact Start -->
<div class="container-fluid py-5 wow fadeInUp" data-wow-delay="0.1s">
    <div class="container py-5">
        <div class="section-title text-center position-relative pb-3 mb-5 mx-auto" style="max-width: 600px;">
            <h5 class="fw-bold text-uppercase">Xe khách bình an</h5>
            <h1 class="mb-0">Liên hệ đặt vé </h1>
        </div>
        <div class="row g-5">
            @foreach(var c in Model)
            {
                <div class="col-lg-4 col-md-6 wow zoomIn" data-wow-delay="0.3s">
                    <div class="service-item bg-light rounded text-center">
                        <div class="service-img">
                            <img src="@c.Images" alt="">
                        </div>
                        <h4 class="mb-3 service-title">@c.Title</h4>
                        <p class="m-0 service-phonte">@c.Phone</p>
                        <a class="btn btn-lg btn-primary rounded" href="">
                            <i class="bi bi-arrow-right"></i>
                        </a>
                    </div>
                </div>
            }
           
        </div>
    </div>
</div>
<!-- Contact End -->